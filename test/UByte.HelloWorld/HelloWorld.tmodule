(module (name "HelloWorld") (format 0 0) (version 0 1 0)
  (identifier $id_namespace "Example")
  (identifier $id_class "HelloWorld")
  (identifier $id_entry_point "main")

  (signature $sig_int (type s32))

  ; Method that takes no parameters and returns a 32-bit integer
  (signature $sig_main (method (results $sig_int)))

  (code $code_main (register $l0 $sig_int) (register $l1 $sig_int)
    (const.i32 9 $l0)
    (const.i32 10 $l1)
    (add $l0 $l1 $l0)
    (ret $l0)
  )

  (namespace (name $id_namespace)
    (type $class_hello_world (name $id_class) (visibility public)
      (class base abstract)
      (method $method_main (name $id_entry_point) (visibility public) (flags static)
        (signature $sig_main)
        (defined $code_main)
      )
    )
  )

  (entrypoint $method_main)
)
