(module (format 0 0) (name "HelloWorld") (version 0 1 0)
  (identifier $id_namespace "Example")
  (identifier $id_class "HelloWorld")
  (identifier $id_entry_point "main")

  (signature $sig_int (type s32))

  ; Method that takes no parameters and returns a 32-bit integer
  (signature $sig_main (method (returns $sig_int)))

  (code $code_main
    (registers
      (local $l0 $sig_int)
      (local $l1 $sig_int))
    ; return 9 + 10
    (const.i32 9 $l0)
    (const.i32 10 $l1)
    (add $l0 $l1 $l0)
    (ret $l0)
  )

  (namespace $namespace_example (name $id_namespace))

  (type $class_hello_world (name $id_class) (namespace $namespace_example)
    (visibility public)
    (class base)
  )

  (method $method_main (name $id_entry_point) (visibility public)
    (signature $sig_main)
    (defined $code_main)
    (owner $class_hello_world)
  )

  (entrypoint $method_main)
)
