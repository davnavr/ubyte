(module (format 0 0) (name "ImportedModule") (version 0 1 0)
  (identifier $id_namespace "Example.Imported")
  (identifier $id_class "MyImport")
  (identifier $id_helper "Helper")

  (signature $sig_int32 (type s32))
  (signature $sig_helper (method (returns $sig_int32) (parameters $sig_int32)))

  (code $code_helper
    (registers (argument $l0 0))

    ; (incr $l0)
    (ret $l0)
  )

  (namespace $namespace (name $id_namespace))

  (type $class_helper (name $id_class) (namespace $namespace)
    (visibility public)
    (class base)
  )

  (method $method_helper (name $id_helper) (visibility public)
    (signature $sig_helper)
    (defined $code_helper)
    (owner $class_helper)
  )
)
