{
  "name": "UByte Text Module",
  "scopeName": "source.txtmdl",
  "fileTypes": [
    "txtmdl"
  ],
  "foldingStartMarker": "\\{",
  "foldingStopMarker": "\\}",
  "patterns": [
    {
      "include": "#comment"
    }
  ],
  "repository": {
    "comment": {
      "begin": "^//.*",
      "beginCaptures": {
        "0": {
          "name": "comment.line.double-slash.txtmdl"
        }
      },
      "end": "\n"
    }
  }
}
